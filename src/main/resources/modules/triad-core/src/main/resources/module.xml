<module version="2.0">
  <meta>
    <identity>
      <uri>MODULE_URN</uri>
      <version>MODULE_VERSION</version>
    </identity>
    <info>
      <name>MODULE_SPACE_NAME</name>
      <description>MODULE_DESCRIPTION</description>
      <icon>MODULE_URN_RES_PATH/images/icon.png</icon>
